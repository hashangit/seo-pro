# SEO Pro Frontend Environment Variables
# Copy this file to .env.local

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
# Publishable key - Safe for client-side (use with RLS)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxxxxxxxxxxxxxxxxxxxxxxx

# WorkOS AuthKit
NEXT_PUBLIC_WORKOS_CLIENT_ID=client_xxxxxxxxxxxxx
NEXT_PUBLIC_WORKOS_REDIRECT_URI=http://localhost:3000

# Backend API
NEXT_PUBLIC_API_URL=http://localhost:8080

# Payment Gateway
# DEV_MODE: When true, skips payment and gives unlimited access
# TODO: Integrate a proper IPG and remove dev mode
NEXT_PUBLIC_DEV_MODE=true
# PayHere integration removed - will be replaced with a new IPG
