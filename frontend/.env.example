# SEO Pro Frontend Environment Variables
# Copy this file to .env.local

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
# Publishable key - Safe for client-side (use with RLS)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxxxxxxxxxxxxxxxxxxxxxxx

# WorkOS AuthKit Next.js Configuration
# Server-side only (never expose to client)
WORKOS_API_KEY=your-workos-api-key-here
WORKOS_CLIENT_ID=client_xxxxxxxxxxxxxxxxxxxxxxxx
WORKOS_COOKIE_PASSWORD=generate-with-openssl-rand-base64-32

# Client-accessible
NEXT_PUBLIC_WORKOS_REDIRECT_URI=http://localhost:3000/callback
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:8080

# Payment Gateway
# DEV_MODE: When true, skips payment and gives unlimited access
# TODO: Integrate a proper IPG and remove dev mode
NEXT_PUBLIC_DEV_MODE=false
# PayHere integration removed - will be replaced with a new IPG
